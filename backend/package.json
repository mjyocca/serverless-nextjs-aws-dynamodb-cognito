{
  "name": "serverless-typescript-template",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node ./src/server.ts",
    "start:test": "TS_NODE_PROJECT=./tsconfig.json node --experimental-specifier-resolution=node --loader ts-node/esm ./src/server.ts",
    "package": "sls package",
    "deploy": "sls deploy --stage dev --force"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.44.0",
    "@tinyhttp/app": "^1.3.7",
    "milliparsec": "1.0.4",
    "serverless-http": "^2.7.0",
    "source-map-support": "^0.5.20"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.62.0",
    "@types/aws-lambda": "^8.10.84",
    "@types/node": "^14.17.27",
    "nodemon": "^2.0.13",
    "serverless": "^2.63.0",
    "serverless-esbuild": "^1.20.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
